<div class="row">
    <h1 class="display-4"> Accounts</h1>
    <div class="col-10 offset-1">
        <table class="table table-dark">
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Deposit</th>
                    <th>Credit Card</th>
                    <th>Profile</th>
                </tr>
            </thead>
            <tbody>
                <% accounts.forEach(account => { %>
                    <tr>
                        <td><%= account.firstName %></td>
                        <td><%= account.lastName %></td>
                        <td><%= account.deposit %></td>
                        <td><%= account.creditCard %></td>
                        <td><a href="#" class="btn btn-sm btn-success">Profile</a></td>
                    </tr>
                <% }) %>
            </tbody>
        </table>
    </div>
</div>